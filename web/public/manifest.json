{
    "name": "Bookmarks",
    "short_name": "Bookmarks",
    "description": "Self-hosted Bookmarks Manager",
    "display": "fullscreen",
    "background_color": "#242424",
    "theme_color": "#242424",
    "start_url": "/index.html",
    "icons": [
        {
            "src": "icons/icon128.png",
            "sizes": "128x128",
            "type": "image/png"
        },
        {
            "src": "icons/icon192.png",
            "sizes": "192x192",
            "type": "image/png"
        },
        {
            "src": "icons/icon256.png",
            "sizes": "256x256",
            "type": "image/png"
        },
        {
            "src": "icons/icon512.png",
            "sizes": "512x512",
            "type": "image/png"
        },
        {
            "src": "icons/icon384.png",
            "sizes": "384x384",
            "type": "image/png"
        }
    ],
    "share_target": {
        "action": "../index.html",
        "method": "GET",
        "enctype": "application/x-www-form-urlencoded",
        "params": {
            "title": "title",
            "text": "text",
            "url": "url"
        }
    },
    "shortcuts": [
        {
            "name": "New Bookmark",
            "short_name": "New Bookmark",
            "description": "New Bookmark",
            "url": "/#/add",
            "icons": [
                {
                    "src": "/icons/plus.png",
                    "sizes": "192x192"
                }
            ]
        },
        {
            "name": "Tags",
            "short_name": "Tags",
            "description": "Tags",
            "url": "/#/tag",
            "icons": [
                {
                    "src": "/icons/tag.png",
                    "sizes": "192x192"
                }
            ]
        }
    ]
}